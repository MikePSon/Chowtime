<p id="notice"><%= notice %></p>

<h1>Listing Create Recipe Categories</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @create_recipe_categories.each do |create_recipe_category| %>
      <tr>
        <td><%= create_recipe_category.name %></td>
        <td><%= link_to 'Show', create_recipe_category %></td>
        <td><%= link_to 'Edit', edit_create_recipe_category_path(create_recipe_category) %></td>
        <td><%= link_to 'Destroy', create_recipe_category, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Create recipe category', new_create_recipe_category_path %>
